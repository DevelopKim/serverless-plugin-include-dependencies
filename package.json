    "posttest": "eslint ."
  },
  "devDependencies": {
    "ava": "0.17.0",
    "babel-polyfill": "^6.23.0",
    "eslint": "3.12.2",
    "nyc": "^10.0.0",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "dependency-tree": "5.9.0",
    "find-root": "1.0.0",
    "lodash": "4.17.2",
    "resolve": "1.2.0",
    "semver": "5.3.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "exclude": [
      "__tests__",
      "coverage"
    ]
  }
}
